  API: resouces/tests.json

=================================

{
    //Array of suites within the scope of the program
    "testSuite": [
        { 
            "dependancyURL": ["array of URL strings"] OR "just a single string",
            "test": [
                {
                    "name": "string",
                    "note": "string",
                    "tag": "string",
                    "firstCanvas" : {
                        "src" : { 
                            "url" : "fileUrl", 
                            "type" : "supported types-> json, script, image" 
                        },
                        "run" : "global function name OR function(aCanvas){ definition }" 
                    },
                    "secondCanvas" : {
                        "src" : { 
                            "url" : "fileUrl", 
                            "type" : "supported types-> json, script, image" 
                        },
                        "run" : "global function name OR function(aCanvas){ definition }" 					
                    }
                },{//More test objects},{...}
            ]
        },{//More testSuite objects},{...}
    ]
}

